{
	"luci-app-echworkers": {
		"description": "Grant access to ECH Workers configuration",
		"read": {
			"file": {
				"/etc/init.d/echworkers": [ "read" ],
				"/sbin/logread": [ "exec" ],
				"/usr/bin/echworkers-gfwlist": [ "exec" ],
				"/usr/bin/echworkers-control": [ "exec" ],
				"/etc/echworkers/*": [ "read" ]
			},
			"ubus": {
				"service": [ "list", "delete" ]
			},
			"uci": [ "echworkers" ]
		},
		"write": {
			"file": {
				"/etc/init.d/echworkers": [ "exec" ],
				"/usr/bin/echworkers-gfwlist": [ "exec" ],
				"/usr/bin/echworkers-control": [ "exec" ],
				"/etc/echworkers/*": [ "write" ]
			},
			"uci": [ "echworkers" ]
		}
	}
}
